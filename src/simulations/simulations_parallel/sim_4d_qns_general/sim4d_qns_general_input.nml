&sim_params
  dt = 0.1
  number_iterations = 0
/
&grid_dims
  num_cells_x1 = 64
  num_cells_x2 = 64
  num_cells_x3 = 32
  num_cells_x4 = 32
/
&mesh_grid
  eta1_min = 0.0
  eta1_max = 1.0

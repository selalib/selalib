  eta2_max = 1.0
  vx_min   = -9.0
  vx_max   =  9.0
  vy_min   = -9.0
  vy_max   =  9.0
/
&space_boundary_conditions
  bc_eta1_0 = 1
  bc_eta1_1 = 1
  bc_eta2_0 = 1
  bc_eta2_1 = 1
